&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME /hppfs/scratch/0E/di76mir/04_convergence_checks_Carlo/02_robust_basis_sets/BASIS_H_B_C_N_O_S_Zn
    POTENTIAL_FILE_NAME ALL_POTENTIALS
    UKS
    MULTIPLICITY 1
    &MGRID
      CUTOFF 400
      REL_CUTOFF 50
    &END MGRID
    &QS
      METHOD GAPW
      EPS_PGF_ORB           1.0E-80
      EPS_FILTER_MATRIX     1.0E-80
    &END QS
    &POISSON
      PERIODIC NONE
      PSOLVER  MT
    &END
    &SCF
      EPS_SCF 1.0E-6
      SCF_GUESS RESTART
      MAX_SCF 30
      &OT
        PRECONDITIONER FULL_SINGLE_INVERSE
        MINIMIZER CG
      &END OT
      &OUTER_SCF
        MAX_SCF 30
        EPS_SCF 1.0E-6
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
      &WF_CORRELATION
        &RI_RPA
          RPA_NUM_QUAD_POINTS  120
          &GW
            CORR_MOS_OCC   10
            CORR_MOS_VIRT  10
            EV_GW_ITER      1
            RI_SIGMA_X
          &END GW
        &END RI_RPA
      &END
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC          52.0 26.0 14.0
      PERIODIC     NONE
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME   gnr.xyz
      COORD_FILE_FORMAT xyz
      &CENTER_COORDINATES
      &END
    &END TOPOLOGY
    &KIND H
      BASIS_SET         aug-cc-pVTZ
      BASIS_SET RI_AUX  aug-cc-pVTZ-RIFIT
      POTENTIAL         ALL
    &END KIND
    &KIND B
      BASIS_SET         aug-cc-pVQZ
      BASIS_SET RI_AUX  aug-cc-pVQZ-RIFIT
      POTENTIAL         ALL
    &END KIND
    &KIND C
      BASIS_SET         aug-cc-pVTZ
      BASIS_SET RI_AUX  aug-cc-pVTZ-RIFIT
      POTENTIAL         ALL
    &END KIND
    &KIND N
      BASIS_SET         aug-cc-pVQZ
      BASIS_SET RI_AUX  aug-cc-pVQZ-RIFIT
      POTENTIAL         ALL
    &END KIND
    &KIND O
      BASIS_SET         aug-cc-pVQZ
      BASIS_SET RI_AUX  aug-cc-pVQZ-RIFIT
      POTENTIAL         ALL
    &END KIND
    &KIND S
      BASIS_SET         aug-cc-pVQZ
      BASIS_SET RI_AUX  aug-cc-pVQZ-RIFIT
      POTENTIAL         ALL
    &END KIND
    &KIND Zn
      BASIS_SET         aug-cc-pVQZ
      BASIS_SET RI_AUX  aug-cc-pVQZ-RIFIT
      POTENTIAL         ALL
    &END KIND
    &KIND C1
      BASIS_SET         aug-cc-pVTZ
      BASIS_SET RI_AUX  aug-cc-pVTZ-RIFIT
      POTENTIAL         ALL
      ELEMENT C
      &BS
        &ALPHA
          NEL 1
          L   1
          N   2
        &END
        &BETA
          NEL -1
          L   1
          N   2
        &END
      &END
    &END KIND
    &KIND C2
      BASIS_SET         aug-cc-pVTZ
      BASIS_SET RI_AUX  aug-cc-pVTZ-RIFIT
      POTENTIAL         ALL
      ELEMENT C
      &BS
        &ALPHA
          NEL -1
          L   1
          N   2
        &END
        &BETA
          NEL 1
          L   1
          N   2
        &END
      &END
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  RUN_TYPE     ENERGY
  PROJECT      benchmark
  PRINT_LEVEL  MEDIUM
  EXTENDED_FFT_LENGTHS
&END GLOBAL
